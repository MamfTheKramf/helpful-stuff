{
    "editor.lineNumbers": "relative",
    "vim.useSystemClipboard": true,
    "vim.leader": " ",
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-y>"
            ],
            "commands": [
                "acceptSelectedSuggestion"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                "selectNextSuggestion"
            ]
        },
        {
            "before": [
                "<C-p>"
            ],
            "commands": [
                "selectPrevSuggestion"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "g",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                "selectNextCodeAction"
            ]
        },
        {
            "before": [
                "<C-y>"
            ],
            "commands": [
                "acceptSelectedCodeAction"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "n"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "f"
            ],
            "commands": [
                "workbench.action.showAllEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "P"
            ]
        },
        {
            "before": [
                "n"
            ],
            "after": [
                "n",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-d>"
            ],
            "after": [
                "<C-d>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-u>"
            ],
            "after": [
                "<C-u>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-f>"
            ],
            "after": [
                "<C-f>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-b>"
            ],
            "after": [
                "<C-b>",
                "z",
                "z"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "P"
            ]
        },
        {
            "before": [
                "n"
            ],
            "after": [
                "n",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-d>"
            ],
            "after": [
                "<C-d>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-u>"
            ],
            "after": [
                "<C-u>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-f>"
            ],
            "after": [
                "<C-f>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-b>"
            ],
            "after": [
                "<C-b>",
                "z",
                "z"
            ]
        }
    ],
    "editor.rulers": [
        120
    ],
    "files.eol": "\n",
    "debug.toolBarLocation": "docked",
    "editor.renderWhitespace": "boundary"
}
